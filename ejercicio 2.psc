Proceso ejercicio2
	n=5
	Dimension a[n]
	Dimension b[n]
	i=1
	Mientras i<=n
		Leer a[i]
		x<-(a[i]*a[i])
		b[i]<-x
		i=i+1
	FinMientras
	i=1
	Imprimir "los elementos del vector a son:"
	Mientras i<=n
		Imprimir Sin Saltar a[i] "-"
		i=i+1
	FinMientras
	Imprimir "*"
	i=1
	Imprimir "los elementos del vector b son:"
	mientras i<=n
		Imprimir Sin Saltar b[i] "-"
		i=i+1
	FinMientras
FinProceso
